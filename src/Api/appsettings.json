{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "YOUR-CONNECTION-STRING"
  },
  "TokenSettings": {
    "SigningKey": "SigningKey",
    "Issuer": "template.api",
    "Audience": "template.app",
    "LifetimeInMinutes": 60
  },
  "MailjetApiSettings": {
    "ApiKey": "ApiKey",
    "ApiSecret": "ApiSecret",
    "Sender": "api@mail-me.com",
    "SenderName": "Template Api Team"
  },
  "FacebookAuthSettings": {
    "AppId": "AppId",
    "AppSecret": "AppSecret"
  }
}
